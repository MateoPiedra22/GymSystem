{
  "name": "gym-management-web",
  "version": "6.0.0",
  "description": "Versión web del Sistema de Gestión de Gimnasios P2P - Dashboard y Reportes",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000 -H 0.0.0.0",
    "build": "next build || exit 0",
    "start": "next start -p 3000 -H 0.0.0.0",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "analyze": "ANALYZE=true next build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "docker:build": "docker build -t gym-frontend .",
    "docker:run": "docker run -p 3000:3000 gym-frontend",
    "docker:dev": "docker-compose -f ../docker-compose.dev.yml up frontend"
  },
  "dependencies": {
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/node": "^20.10.5",
    "tailwindcss": "^3.3.6",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "recharts": "^2.10.1",
    "chart.js": "^4.4.1",
    "react-chartjs-2": "^5.2.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "lucide-react": "^0.300.0",
    "date-fns": "^3.0.6",
    "clsx": "^2.0.0",
    "axios": "^1.6.2",
    "next-themes": "^0.2.1",
    "@tanstack/react-query": "^5.14.2",
    "zod": "^3.22.4",
    "jose": "^5.2.0",
    "js-cookie": "^3.0.5",
    "@types/js-cookie": "^3.0.6",
    "class-variance-authority": "^0.7.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "framer-motion": "^10.17.9",
    "react-hot-toast": "^2.4.1",
    "numeral": "^2.0.6",
    "@types/numeral": "^2.0.5"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "@typescript-eslint/parser": "^6.17.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "@testing-library/react": "^14.1.2",
    "@testing-library/jest-dom": "^6.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
